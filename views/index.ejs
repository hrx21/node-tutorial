<html lang="en">
<%- include('./partials/head.ejs') %> 
<body>
    <h3>Blog Website</h3>
  <%- include('./partials/nav.ejs') %> 
    <div>
        <% if (blogs.length > 0) { %>
            <% blogs.forEach(blog => { %>
              <h3 class="title"><%= blog.title %></h3>
              <p class="snippet"><%= blog.snippet %></p>
            <% }) %>
          <% } else { %>
            <p>There are no blogs to display...</p>
          <% } %>
    </div>
    <%- include('./partials/footer.ejs') %> 
</body>
</html>